{
  "data_security_mode": {
    "type": "fixed",
    "value": "SINGLE_USER",
    "hidden": true
  },
  "single_user_name": {
    "type": "regex",
    "pattern": "([a-zA-Z0-9+._-]+@[a-zA-Z0-9._-]+\\.[a-zA-Z0-9_-]+)",
    "hidden": true
  },
  "spark_version": {
    "type": "regex",
    "pattern": "10\\.[1-9].*",
    "defaultValue": "10.3.x-scala2.12"
  }
}